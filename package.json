{
  "name": "usockets",
  "version": "0.0.6",
  "description": "Universal socket.io wrapper with promise and request/response support",
  "license": "MIT",
  "browser": "./browser.js",
  "scripts": {
    "test": "mocha test/ test/**/*.js",
    "webpack": "webpack --progress --watch --display-error-details .",
    "build": "webpack -p ."
  },
  "babel": {
    "presets": [
      "stage-3",
      "es2015"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "transform-object-rest-spread",
      "transform-runtime",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "react/prefer-es6-class": 0,
      "react/prefer-stateless-function": 0,
      "react/jsx-no-bind": 0,
      "react/sort-comp": 0,
      "no-trailing-spaces": 0,
      "object-curly-spacing": 0,
      "no-param-reassign": 0,
      "arrow-body-style": 0,
      "no-console": 0
    }
  },
  "dependencies": {
    "socket.io": "2.0.3",
    "socket.io-client": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-flow-strip-types": "6.4.0",
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-runtime": "6.23.0",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-plugin-jsx-a11y": "0.6.2",
    "eslint-plugin-react": "4.3.0",
    "esdoc": "https://github.com/Flux159/esdoc.git#15b194e1510f1ae8be687fa5cee30fa6f66b6a35",
    "esdoc-node": "1.0.0",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "webpack": "2.2.1"
  }
}
